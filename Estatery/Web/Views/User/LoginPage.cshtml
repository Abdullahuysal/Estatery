@model Dtos.Requests.UserLoginRequest
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <link href="~/css/login.css" rel="stylesheet" />
    <title>LoginPage</title>
</head>
<body>
    <form asp-action="LoginPage" method="post">
        <div class="form">
            <h1>Real Estatery Giriş Yap</h1>
            <input asp-for="Email" class="text_box" placeholder="E-mail Adres(example@gmail.com)" type="email"/>
            <input asp-for="Password" class="text_box" placeholder="Şifre" type="password"/>
            <div class="form-group">
                <input  class="submit-button" type="submit" value="Giriş Yap"/>
            </div>

        </div>
    </form>
</body>
</html>
